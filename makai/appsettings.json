{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DrawControllerConfig" : {
    "DrawingsFolder" : "drawings",
    "ArtistData" : "data.json",
    "GeneralSafetyRegex" : "^[a-zA-Z0-9_\\-]+"
  },
  "ChatlogControllerConfig" : {
    "ShellProgram" : "bash",
    "ChatlogLocation" : "chatlogs",
    "IncludeRegex" : "^[a-zA-z0-9\\-\\*]*$",
    "MaxWait" : "00:30"
  },
  "RenderConfig" : {
    "TemplateLocation" : "Templates",
    "TemplateExtension" : ".mustache"
  }
}
